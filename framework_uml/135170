      operation 177282 "GetInstance"
	class_operation public explicit_return_type "object"
	nparams 4
	  param inout name "host" explicit_type "<type>"
	    defaultvalue "null"
	  param inout name "user" explicit_type "<type>"
	    defaultvalue "null"
	  param inout name "pass" explicit_type "<type>"
	    defaultvalue "null"
	  param inout name "db" explicit_type "<type>"
	    defaultvalue "null"
	
	
	preserve_php_body_indent php_def "  ${comment}${final}${visibility}${abstract}${static}function ${name}${(}${p0}${v0}, ${p1}${v1}, ${p2}${v2}, ${p3}${v3}${)}
{
  ${body}}
"
	
	
	comment "
Used for retreiving an instance of a singleton if wanted
@deprecated since version 3.0 in favor for GetInstance
@return ${type} DB

public static function getInstance($host=null, $user=null, $pass=null, $db=null) {
return Db::GetInstance($host, $user, $pass, $db);
}

Used for retreiving an instance of a singleton if wanted
@global <type> $config
@param ${t0} ${p0}
@param ${t1} ${p1}
@param ${t2} ${p2}
@param ${t3} ${p3}
